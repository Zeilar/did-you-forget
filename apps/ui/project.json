{
  "name": "ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ui",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project ui --web",
  "targets": {
    "theme-types": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx @chakra-ui/cli typegen apps/ui/src/theme/index.ts"
      }
    },
    "generate-vapid-keys": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node apps/ui/setup/generate-vapid-keys.ts"
      }
    },
    "dev-certs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nx dev ui --experimental-https"
      }
    }
  }
}
